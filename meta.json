{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "viam-viz:draw-tools",
  "visibility": "private",
  "url": "https://github.com/viam-labs/draw-tools",
  "description": "Tools to support visualizations.",
  "models": [
    {
      "api": "rdk:service:world_state_store",
      "model": "viam-viz:draw-tools:draw-arrows",
      "short_description": "Allows drawing poses as arrows.",
      "markdown_link": "README.md#model-viam-vizdraw-toolsdraw-arrows"
    },
    {
      "api": "rdk:component:button",
      "model": "viam-viz:draw-tools:clear-arrows-button",
      "short_description": "Clears all arrows from the world state.",
      "markdown_link": "README.md#model-viam-vizdraw-toolsclear-arrows-button"
    },
    {
      "api": "rdk:component:button",
      "model": "viam-viz:draw-tools:draw-arrows-button",
      "short_description": "Draws arrows from the world state.",
      "markdown_link": "README.md#model-viam-vizdraw-toolsdraw-arrows-button"
    },
    {
      "api": "rdk:service:world_state_store",
      "model": "viam-viz:draw-tools:draw-mesh-world-state",
      "short_description": "Allows drawing 3D meshes from PLY files.",
      "markdown_link": "README.md#model-viam-vizdraw-toolsdraw-mesh-world-state"
    },
    {
      "api": "rdk:component:button",
      "model": "viam-viz:draw-tools:clear-mesh-button",
      "short_description": "Clears all meshes from the world state.",
      "markdown_link": "README.md#model-viam-vizdraw-toolsclear-mesh-button"
    },
    {
      "api": "rdk:component:button",
      "model": "viam-viz:draw-tools:draw-mesh-button",
      "short_description": "Draws a mesh from a specified PLY file path.",
      "markdown_link": "README.md#model-viam-vizdraw-toolsdraw-mesh-button"
    }
  ],
  "applications": null,
  "markdown_link": "README.md",
  "entrypoint": "bin/draw-tools",
  "first_run": "",
  "build": {
    "build": "make module.tar.gz",
    "setup": "make setup",
    "path": "module.tar.gz",
    "arch": ["linux/amd64", "linux/arm64", "darwin/arm64", "windows/amd64"]
  }
}
